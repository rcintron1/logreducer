    "version": "0.2.0",
    "configurations": [




        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Testing Options",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": ["--help"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Testing Existing Local File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": ["-f","kafkaServer.out"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Testing Non-Existing Local File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": ["-f","nonexistingfile.out"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Testing Docker Local File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": ["-f","/var/log/docker"]
        }
    ]
}